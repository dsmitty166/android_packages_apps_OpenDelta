[project]
	description = OpenDelta was written to provide automatic OTA updates for The OmniROM Project's nightly builds, making use of deltas when possible, to reduce the size of the download.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/David Smit (dsmitty166)
[access "refs/for/refs/*"]
	read = group user/David Smit (dsmitty166)
	push = group user/David Smit (dsmitty166)
	label-Code-Review = -2..+2 group user/David Smit (dsmitty166)
	label-Verified = -1..+1 group user/David Smit (dsmitty166)
	rebase = group user/David Smit (dsmitty166)
	removeReviewer = group user/David Smit (dsmitty166)
	submit = group user/David Smit (dsmitty166)
[access "refs/heads/*"]
	read = group user/David Smit (dsmitty166)
	create = group user/David Smit (dsmitty166)
	push = +force group user/David Smit (dsmitty166)
	pushMerge = group user/David Smit (dsmitty166)
[access "refs/tags/*"]
	push = +force group user/David Smit (dsmitty166)
	pushTag = group user/David Smit (dsmitty166)
	pushSignedTag = group user/David Smit (dsmitty166)
